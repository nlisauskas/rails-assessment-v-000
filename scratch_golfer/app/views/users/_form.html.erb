<%= form_for @user do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<br></br>
<%= f.label :hometown %>
<%= f.text_field :hometown %>
<br></br>
<%= f.label :password %>
<%= f.text_field :password %>
<br></br>
<%= f.label :password_confirmation %>
<%= f.text_field :password_confirmation %>
<br></br>
<%= f.submit %>
<% end %>
